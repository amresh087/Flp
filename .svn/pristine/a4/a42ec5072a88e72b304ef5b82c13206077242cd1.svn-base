/** Globel css **/
html, body, div, table, img, br, p, tr, td, tbody, dd, dr, ul, li, ol, header, footer, aside, article, section, nav, figure, 
figurecaption, audio, video, iframe, embed, dir, label, input, select, option, datalist, button, form, pre, code, kbd, samp, span
{margin:0px; padding:0px; outline:none;}

html{height:100%;}
Body{ width:100%; font-family: 'Arial', sans-serif !important; font-size:13x; font-weight:normal; color:#6f6f6f; line-height:18px;}

header, footer, nav, aside, article, section, figure, figurecaption, audio, video{ display:block;}
img, embed, audio, video, object, frameborder { border:0px !important;} 
	
input[type="text"], input[type="password"], input[type="date"], input[type="email"], input[type="time"],
input[type="datalist"], textarea { width:100%; border:none; padding:6px 5px; vertical-align:middle; 
font-family: 'Arial', sans-serif !important; color:#999; }

input[type="radio"]{}
select{ width:100%; border:#CCCCCC solid 1px; border-radius:3px; padding:0px 5px; height:30px; vertical-align:middle;  
font-family: 'Arial', sans-serif !important; color:#999; }

option{ padding:3px 5px;}

input[type="text"], input[type="password"], input[type="date"], input[type="email"], input[type="time"],
input[type="datalist"]{ border:#CCCCCC solid 1px; font-size:13px;}

 input[type="radio"]{font-size:13px;}

textarea{ border:#CCC solid 1px; }

a{color:#000; text-decoration:none;}	
a:hover{color:#ea6c00; text-decoration:none;}
a:active{color:#039; text-decoration:none !important;}

ul, ol, li{}
ul li{ margin-bottom:0px;}

h1,h2,h3,h4,h5,h6{font-weight:normal;  margin:0px; padding:0px; font-family: 'Arial', sans-serif !important;}

h1{ font-size:16px; text-transform:uppercase; font-family: 'Arial', sans-serif !important; color:#cb3536; padding:15px 0;}

h2{color:#df5820; font-size:24px; font-weight:normal; padding:15px 0; background:url(../../images/heading_br.png) left 
bottom repeat-x; }
h3{color:#226a94; font-size:23px; font-weight:bold; padding:10px 15px;}
h4{font-size:14px; padding:3px 0; color:#8b2c13; }
h5{font-size:16px; padding:7px 0; color:#104577;}
h5 span{font-size:14px; padding:7px 0; color:#000;}
h6{font-size:13px; padding:3px 0; color:#DE7500; line-height:20px; }

p{padding:0px 0;}
strong{ color:#6f6f6f;font-size:12px; font-family:Arial, Helvetica, sans-serif; }

.clearfix:after, .clearfix:before{ clear:both; display:block; content:""; height:0px; overflow:hidden;} 
.clr{clear:both;}
/** Globel css end **/ 

