@media (max-width:768px) {
.fot-pd{padding-left:20%;}
.student-board-bt{ padding:5px 25px;}
.topic-question-name{ width:56%; font-size:13px;}
.pro-bar-cont p{ font-size:12px;}
.pro-bar-cont{ width:90px;}
.side-head{ font-size:15px; }
.sub-side-cont li a{ padding:10px; font-size:13px;}
.question-panel li{ padding:2% 3%; margin-right:3px; border:#CCC solid 1px; font-size:18px; background:#f2f2f2;}
.currentscore, .resource{ margin-right:0px;}
.timerr ul li{width:27%; }

.sum-ques-left{ float:left; width:5%; text-align:center;  }
.sum-ques-middle{  float:left; width:88%; }
.sum-ques-right{  float:left; width:7%;  }

/* student Retail User */
.stu-cont{ padding:30px 0;}
.stu-zone p{ padding:15px; font-size:16px;}
.stu-board-name{ width:100%;}
.stu-board-con{ left:15%; width:70%; }

/* parent user */
.stu-pro-active{ padding:0px 13px;}
.sel-ch-active{ padding:30px 20px  50px 20px}
.report > li > a{ padding:17px 6px; font-size:12px;}
.stu-profile-cont{ margin:0 9px;}
.sel-ch-active select{ height:46px;}
.parent-content-content2{ width:51%;}
.parent-content-content-gscore2{ width:22%;}
}

